<div class="center-block video-upload">
<%= form_for @video do |form| %>
  <div class="form-group">
    <%= form.file_field :video, class: ['btn' 'btn-default' 'btn-file'] %>
  </div>
  <div class="form-group">
    <%= form.label :latitude, 'Latitude' %>
    <%= form.text_field :latitude, value: '' %>
  </div>
  <div class="form-group">
    <%= form.label :latitude, 'Longitude' %>
    <%= form.text_field :longitude, value: '' %>
  </div>
  <div class="form-group">
    <%= form.submit :Upload, class: ['btn-lg', 'btn-primary'] %>
  </div>
<% end %>
</div>
